		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "7.3.6",
		"@mui/material": "7.3.6",
		"@mui/material-nextjs": "7.3.6",
		"ical-generator": "^10.0.0",
		"next": "16.1.1",
		"node-html-markdown": "^2.0.0",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-gtm-module": "^2.0.11",
		"sharp": "^0.34.5",
		"yaml": "^2.8.2"
