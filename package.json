{
	"name": "gdscutm-website",
	"description": "GDSC UofT Mississauga Website",
	"version": "2023.10",
	"private": true,
	"license": "LGPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "https://github.com/utmgdsc/website.git"
	},
	"bugs": {
		"url": "https://github.com/utmgdsc/website/issues"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@mui/icons-material": "^5.15.15",
		"@mui/material": "^5.15.15",
		"js-yaml": "^4.1.0",
		"next": "^14.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-gtm-module": "^2.0.11",
		"sharp": "^0.33.3"
	},
	"devDependencies": {
		"eslint": "^9.3.0",
		"eslint-config-next": "^14.1.3",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.34.1",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5"
	},
	"scripts": {
		"start": "next start",
		"build": "next build",
		"dev": "next dev",
		"lint": "next lint",
		"prepare": "husky",
		"pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css}\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSameLine": false,
		"bracketSpacing": true,
		"endOfLine": "auto",
		"jsxSingleQuote": false,
		"printWidth": 120,
		"quoteProps": "as-needed",
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	}
}
